  "devDependencies": {
    "cordova-android": "^9.1.0",
    "cordova-ios": "^6.2.0",
    "cordova-plugin-androidx-adapter": "^1.1.3",
    "cordova-plugin-camera": "git+https://github.com/ryaa/cordova-plugin-camera.git",
    "cordova-plugin-geolocation": "^4.1.0",
    "cordova-plugin-inappbrowser": "^5.0.0",
    "cordova-plugin-migrate-localstorage": "git+https://github.com/viglino/cordova-plugin-migrate-localstorage.git",
    "cordova-plugin-splashscreen": "^6.0.0",
    "cordova-plugin-wkwebview-file-xhr": "^3.0.0",
