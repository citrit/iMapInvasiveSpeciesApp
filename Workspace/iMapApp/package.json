      "cordova-plugin-wkwebview-file-xhr": {},
      "cordova-plugin-androidx-adapter": {},
      "cordova-plugin-migrate-localstorage": {}
    }
  },
  "dependencies": {
    "cordova-custom-config": "^5.1.0",
    "cordova-plugin-compat": "^1.2.0",
